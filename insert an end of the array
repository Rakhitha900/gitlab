#include<stdio.h>
#define CAP 10
int main()
{
    int arr[]={1,3,4,5,6};
    int n = 3;
    int value = 3;
    if(n=CAP)
    {
        arr[n]=value;
        n++;
    }
    else
    {
        printf("array is full\n");
    }
    for( int i=0; i<n; i++)
    printf("%d",arr[i]);
    printf("\n");
    return 0;
}
