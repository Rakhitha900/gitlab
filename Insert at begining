#include<stdio.h>
#define CAP 10
int main()
{
    int a[CAP] = {1,2,3,4};
    int n = 5;
    int x = 7;
    if(n < CAP)
    {
        for(int i=n; i>0; i--)
        {
            a[i]=a[i-1];
        }
        a[0] = x;
        n++;
    }
    else
    {
        printf("arr full");
    }
    for(int i=0; i<n; i++)
    printf("%d ",a[i]);
    printf("\n");
    return 0;
}
