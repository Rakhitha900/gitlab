//delete at index
#include<stdio.h>
int main()
{
    printf("NAME:RAKSHITHA\n USN:1SV24CS115\n");
    int arr[5]={2,4,6,8,5};
    int n,i;
    n=5;
    int pos = 2;
    for(i=pos; i<n-1; i++)
    {
        arr[i]=arr[i +1];
    }
    n--;
    for(i=0; i<n; i++)
    printf("arr[%d]=%d\n",i,arr[i]);
    return 0;
}
