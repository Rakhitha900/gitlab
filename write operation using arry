#include<stdio.h>
int main()
{
    int arr[]={2,3,4,5,6};
    int n = sizeof(arr)/sizeof(arr[0]);
    int index=20;
    int newvalue = 10;
    if(index>=0 && index<n)
    {
        arr[index]=newvalue;
        for(int i=0;i<n;i++)
        {
            printf("%d ",arr[i]);
        }
    }
    else
    {
        printf("Hello world\n");
    }
    return 0;
}
