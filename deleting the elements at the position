//deleting the element at the position given
#include<stdio.h>
#define CAP 50
int main()
{
    printf("NAME = RAKSHITHA\n");
    printf("USN = 1SV24CS115\n");
    int a[CAP]={1,2,3,4,5,6};
    int n=6;
    int pos = 4;
    if(n>0 && pos >=0 && pos<n)
    {
        for(int i=pos; i<n-1; i++)
        {
            a[i]=a[i+1];
        }
        n--;
    }
    else
    {
        printf("Invalid");
    }
    for(int i=0;i<n;i++)
    printf("%d ",a[i]);
}
